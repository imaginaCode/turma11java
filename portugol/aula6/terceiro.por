programa
{

	funcao pularLinha(){
		
		escreva ("\n")
		}

	
	
	funcao inicio()
	{

		cadeia nomeUsuario // declara variaveis do tipo texto, variavel nao pode começar com numero nem com espaço

		
		pularLinha()
		escreva("Olá Mundo ")
		pularLinha()
		leia (nomeUsuario)
		escreva("meu nome é " + nomeUsuario)
		pularLinha()
		escreva("sou alune da Generation")
	}
}
